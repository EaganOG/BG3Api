// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Armour{
  id Int @id @default(autoincrement())
  name String
  type String
  rarity String
  effect1 String
  effect2 String
  value Int
  ac Int
  proficiency Int
}
